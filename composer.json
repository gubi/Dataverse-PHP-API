{
    "name": "dataverse/php-library",
    "description": "A PHP wrapper around the Dataverse API",
    "homepage": "https://github.com/gubi/dataverse-php-library",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "dataverse", "api", "php", "library"
    ],
    "authors": [
        {
            "name": "gubi",
            "email": "a.gubitosi@cgiar.org"
        }
    ],
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.2.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2",
        "php-curl": "*",
        "php-json": "*",
        "php-xml": "*",
        "php-zip": "*"
    },
    "autoload": {
        "psr-4": {
            "Dataverse\\": "src/"
        }
    }
}
